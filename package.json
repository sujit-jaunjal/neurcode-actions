{
  "name": "neurcode-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:cli": "pnpm --filter @neurcode-ai/cli dev",
    "build": "pnpm -w -r build",
    "build:cli": "pnpm --filter @neurcode-ai/cli build",
    "build:parser": "pnpm --filter @neurcode-ai/diff-parser build",
    "build:action": "pnpm --filter @neurcode/github-action build",
    "build:verify-action": "pnpm --filter @neurcode/action build",
    "cli": "pnpm --filter @neurcode-ai/cli start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {}
}
